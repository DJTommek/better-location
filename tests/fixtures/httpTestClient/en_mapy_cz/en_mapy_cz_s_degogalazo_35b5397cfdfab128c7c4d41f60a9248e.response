<script>
	(function() {
		var compress =  true ;
		var TLD = "cz";
		var DOMAIN = "mapy";
		var requestUrl = "https://pro.mapy.cz";
		window.MapyConfig = {
			version: "2.62.8",
			jsCompress: compress,
			apiUrl: "https://api.mapy.cz",
			TLD: TLD,
			DOMAIN: DOMAIN,
			mapyURL: "https://" + (Userweb.defaultLang != "cs" ? Userweb.defaultLang + "." : "") + DOMAIN +"." + TLD,
			frameUrl: "https://" + (Userweb.defaultLang != "cs" ? Userweb.defaultLang + "." : "") + "frame." + DOMAIN +"." + TLD,
			requestUrl: requestUrl,
			loginUrl: "https://login.mapy.cz",
			media: requestUrl + "/mediarpc",
			april19URL: "https://staredobre.mapy.cz",
			authInterval: parseInt("120000", 10) || 120000,
			loadMapLoginTimeout: parseInt("2000", 10) || 2000,
			
			inFrame: false,
			UserwebSetting: JSON.parse('{"userweb":{"firmDebug":0,"abTestsString":"","firmy-search-in-reviews":0,"actualDate":"","deepllang":"","newDetail":0,"badgeWCMapGreen":0,"routeNewHD":0,"grpcLocation":0,"grpcLocationDebug":0,"generalSharePosition":0,"wasm":0,"bookingLink":1,"twinsReplaceId":1},"ribbon":{"enabled":0,"debug":0,"user":0},"search":{"showProfileQuality":0,"titleTextBlockWidth":125,"searchLimitOnPage":15,"searchLocationBBoxTolerance":0,"searchLimit":200,"searchViewport":0,"searchViewportBackend":0,"searchViewportTop":70,"searchViewportRight":12,"searchViewportBottom":58,"searchViewportLeft":12},"review":{"webcomponent":0},"route":{"anotherCriterion":0,"azimuth":0,"datetimeForAll":0},"lang":{"rusia":0,"franc":0,"spain":0,"italian":0},"mapybox":{"limitOnPage":50},"gallery":{"limit":100},"history":{"sid":1024},"editorial":{"interface":0}}'),
			rating: parseFloat('0'),
			filesVersion: "2.62.8",
			environment: "stable",
			initialMap: {
				lon: 15.625233,
				lat: 49.8022514,
				zoom: 8,
			},
			initialMapset: "zakladni",
			mapSuppression: true,
			userHistory: {},
			mapsets: [],
			mapsetsIDsMenuList: [1,2,21,7,3],
			mapsetsBlackList: ["hapticka","atlas"],
			
			
			
			
				dontShowAndroidPromo: 0,
			
			abCountry: "DE",
		};
		// config pro grpc, geo lokace [m]
		window.GEO_CONFIG = {
			x: 60,
			y: 60 * 8,
		};

		MapyConfig.Left3d = {
	url: "https://mapserver-3d.mapy.cz/scenes/latest/mapConfig.json",
	allowed:1
};


		// hlavni callback pro userweb js
		var loadCb = function() {
			
			if (window.geo) {
				geo.conf.showDialogs = true;
			}
			

			if ("login" in window) {
				const loginCFG = {
					serviceId: "mapy",
					lang: ["cs", "sk", "de", "uk"].indexOf(Userweb.defaultLang) > -1 ? Userweb.defaultLang : "en",
					onDebugChange: show => {
						Mapy.getComponent("profile").checkDebug(show);
					},
				};

				if (window.MapyConfigBE?.abTest?.flags.includes("login")) {
					loginCFG.test = `${window.MapyConfigBE?.abTest.flags}_DE`;
				}

				login.cfg(loginCFG);
			}

			SMap.CONFIG.pano.compose = "//panorama-mapserver.mapy.cz/compose_pano";

			


			
			(function() {
	window.addEventListener("load", function() {
		Mapy.showAndroidAppPromotion();
	});
})();

			

			try {
				Mapy.init().then(function() {
				
				}, function() {});
			} catch (exc) {
				console.log("Init exception");
				console.log(exc);
			}


			<!-- GEMIUS --><!-- analytika si po nacteni upravi pp_gemius_identifier - prida na zacatek USED_, my to chceme volat bez, proto se bude pouzivat promenna s velkymi pismeny -->

	window.PP_GEMIUS_IDENTIFIER = "ofVA4j_YDzfC81mdmfx2KMVHfeC1heOUDvVvHfpn10..H7";

window.pp_gemius_identifier = window.PP_GEMIUS_IDENTIFIER;
window.pp_gemius_extraparameters = ["gA=mapy.cz/mapy.cz/homepage"];
window.pp_gemius_use_cmp = false;
window.pp_gemius_gdpr_consent = window.MapyConfigBE?.locationMeta?.euconsentV2 || "";

function gemius_pending(i) { window[i] = window[i] || function() {var x =
window[i+'_pdata'] = window[i+'_pdata'] || []; x[x.length]=arguments;};};
gemius_pending('gemius_hit'); gemius_pending('gemius_event');
gemius_pending('pp_gemius_hit'); gemius_pending('pp_gemius_event');
(function(d,t) {try {var
gt=d.createElement(t),s=d.getElementsByTagName(t)[0];
gt.setAttribute('async','async'); gt.setAttribute('defer','defer');
gt.src=location.protocol+'//gacz.hit.gemius.pl/xgemius.js';
s.parentNode.insertBefore(gt,s);} catch (e) {}})(document,'script');

		};

		// nacteme async api
		if (window.MapyIsLegacy) {
			var legacySrc = "/js/ie11-banner.2.62.8.js?" + (compress ? "" : Math.random());

			loadScript(legacySrc, function() {
				if (window.MapyczIE11Banner) {
					MapyczIE11Banner.config({
						enabled: true,
						variant: 3,
						lang: "en",
						force: true,
						styleUrl: "/css/ie11-banner.2.62.8.css",
					});
				}
			});
		} else {
			function loadScriptProm(src, crossorigin) {
				return new Promise(resolve => {
					const script = document.createElement("script");

					script.src = src;
					script.type = "text/javascript";
					script.async = true;
					script.onload = function() {
						resolve();
					};

					if (crossorigin) {
						script.setAttribute("crossorigin", "");
					}

					document.head.appendChild(script);
				});
			}

			function loadApi() {
				return new Promise(resolve => {
					loadScriptProm("https://api.mapy.cz/v5/loader.js", true).then(() => {
						
						Loader.async = true;
						// nastavime jazyk do api
						Loader.lang = Userweb.defaultLang;
						// nacteme async soubory s api
						Loader.load(null, { poi:true, pano:true, suggest: true }, function() {
							resolve();
						});
					});
				});
			}

			function loadMapData() {
				return new Promise(resolve => {
					let grpc = false;

					// klikatko - kdyz je nastavene, hrabeme do kese
					try {
						const userwebConfigSettings = sessionStorage.getItem("UserwebConfigSettings");

						if (userwebConfigSettings) {
							const json = JSON.parse(userwebConfigSettings);

							grpc = json.userweb?.grpcLocation === 1 || json.userweb?.grpcLocationDebug === 1;
						}
					} catch (exc) {
					}

					// pro responzive
					const mobile = window.innerWidth <= 789;
					const usp = new URLSearchParams();

					usp.append("lang", Userweb.defaultLang);
					usp.append("grpc", grpc ? 1 : 0);
					usp.append("mobile", mobile ? 1 : 0);
					// minuty - upravy
					usp.append("x", GEO_CONFIG.x);
					usp.append("y", GEO_CONFIG.y);

					fetch(`/mapLoad?${usp.toString()}`).then(response => {
						response.json().then(json => {
							resolve(json);
						}, () => resolve(null));
					}, () => resolve(null));
				});
			}

			const userwebSrc = `/js/userweb.2.62.8.js?${compress ? "" : Math.random()}`;

			Promise.allSettled([
				loadMapData().then(mapData => {
					window.MapyConfigBE = mapData;
				}, () => {}),
				loadApi(),
			]).then(() => {
				loadScriptProm(userwebSrc).then(() => {
					loadCb();
				}, () => {
					// nenacetl se userweb
				});
			});
		}

		// ! zakazani nacitani scmp pres ssp.js
		window._sspDoNotLoadCMP = true;
	})();
</script>



