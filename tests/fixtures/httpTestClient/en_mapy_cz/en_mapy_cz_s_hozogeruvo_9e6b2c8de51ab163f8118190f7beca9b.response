	function loadApi() {
		return new Promise(resolve => {
			window.MapyHead.loadScript("https://api.mapy.cz/v5/loader.js", true).then(() => {
				if (!window.MapyConfig.apiCompress) {
					Loader.mode = "multi";
				}

				Loader.async = true;
				// nastavime jazyk do api
				Loader.lang = Userweb.defaultLang;
				// nacteme async soubory s api
				Loader.load(null, { poi:true, pano:true, suggest: true }, function() {
					// TODO potom odebrat, az bude komplet prechod hotovy
					SMap.CONFIG.mapyczDomain = "https://mapy.com/en";
					SMap.CONFIG.mapyUrl.route = "https://mapy.com/en/?planovani-trasy";
					resolve();
				});
			});
		});
	};

	function loadMapData() {
		return new Promise(resolve => {
			let grpc = false;

			// klikatko - kdyz je nastavene, hrabeme do kese
			try {
				const userwebConfigSettings = sessionStorage.getItem("UserwebConfigSettings");

				if (userwebConfigSettings) {
					const json = JSON.parse(userwebConfigSettings);

					grpc = json.userweb?.grpcLocation === 1 || json.userweb?.grpcLocationDebug === 1;
				}
			} catch (exc) {
			}

			// pro responzive
			const mobile = window.innerWidth <= window.MapyConfig.geoConfig.mobileMinWidth;
			const usp = new URLSearchParams();
			const curUsp = new URLSearchParams(location.search);
			const country = curUsp.get("custom-country") || "";

			usp.append("lang", Userweb.defaultLang);
			usp.append("grpc", grpc ? 1 : 0);
			usp.append("mobile", mobile ? 1 : 0);
			// minuty - upravy
			usp.append("x", window.MapyConfig.geoConfig.x);
			usp.append("y", window.MapyConfig.geoConfig.y);

			if (country) {
				usp.append("country", country);
			}

			fetch(`/mapLoad?${usp.toString()}`).then(response => {
				response.json().then(json => {
					resolve(json);
				}, () => resolve(null));
			}, () => resolve(null));
		});
	};

	if (window.MapyHead.isLegacy) {
		const legacySrc = "/js/ie11-banner.2.74.5.js?" + (window.MapyConfig.jsCompress ? "" : Math.random());

		window.MapyHead.loadScript(legacySrc).then(() => {
			if (window.MapyczIE11Banner) {
				MapyczIE11Banner.config({
					enabled: true,
					variant: 3,
					lang: "en",
					force: true,
					styleUrl: "/css/ie11-banner.2.74.5.css",
				});
			}
		});
	} else {
		const userwebSrc = `/js/userweb.2.74.5.js?${window.MapyConfig.jsCompress ? "" : Math.random()}`;

		Promise.allSettled([
			loadMapData().then(mapData => {
				window.MapyConfigBE = mapData;
			}, () => {}),
			loadApi(),
		]).then(() => {
			window.MapyHead.loadScript(userwebSrc).then(() => {
