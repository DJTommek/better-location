function handleUploadedPhotos(results) {
  document.getElementById('add-photo-loader').style.display = 'none'

  var result;
  for (var i = 0, ii = results.length; i < ii; ++i) {
    result = results[i];
    if (result['error']) {
      alert('Vaše fotografie nebyla přidána (chyba ' + result['error'] + ')');
    } else {
      pushPhoto(result);
            alert('Vaše fotografie byla přidána a čeká na schválení.');
          }
  }
};
function pushPhoto(photo) {
  var noPhotos = document.getElementById('no-photos');
  if (noPhotos) {
    noPhotos.parentNode.removeChild(noPhotos);
  }

  var ul = document.getElementById(photo['zobrazit'] ? 'photos' : 'photos-queue');
  var li = document.createElement('li');

  var size_qs = photo['zobrazit'] ? (ul.childNodes.length ? 'w=78&h=78' : 'w=240') : 'w=78&h=78';

  if (!photo['zobrazit']) {
    document.getElementById('photos-queue-title').style.display = 'block';
  }

  var a = document.createElement('a');
  a.className = 'image-browser-source';
  a.href = './image.php?id=' + photo['id'] + '&h=800';
  a.title = photo['popis'];
  a.setAttribute('data-browser', 'photos');

  var note = [];
  if (photo['datum']) {
    note.push('datum pořízení fotografie: ' + photo['datum']);
  }
  if (photo['zdroj']) {
    note.push('zdroj: ' + photo['zdroj']);
  }
  if (photo['prispel'] && photo['prispel'] !== photo['zdroj']) {
    note.push('nahrál: ' + photo['prispel']);
  }
  a.setAttribute('data-note', note.join(' – '));

  var img = new Image();
  img.src = './image.php?id=' + photo['id'] + '&' + size_qs;
  a.appendChild(img);
  li.appendChild(a);
  ul.appendChild(li);

  ui.ImageBrowser.findSetsOnPage();
};

