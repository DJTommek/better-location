<script>
    $(document).ready(function()
    {
        let imageInput = $('#frm-addImageForm-form-image');
        let previewImage = $('#preview_image');

        imageInput.change(function() {
            // Zkontroluj, zda byl vybrán soubor
            if (this.files && this.files[0]) {
                // Vytvoř objekt URL pro náhled zvoleného obrázku
                let reader = new FileReader();

                reader.onload = function(e) {
                    // Nastav src atribut náhledového obrázku na URL zvoleného obrázku
                    previewImage.attr('src', e.target.result);
                    previewImage.parent().removeClass('d-none');
                }

                // Přečti obsah zvoleného souboru jako URL
                reader.readAsDataURL(this.files[0]);
                $('label[for="'+this.id+'"].custom-file-label').text(this.files[0].name);
            } else {
                $('label[for="'+this.id+'"].custom-file-label').text('Soubor nevybrán');
            }
        });
    })
</script>
