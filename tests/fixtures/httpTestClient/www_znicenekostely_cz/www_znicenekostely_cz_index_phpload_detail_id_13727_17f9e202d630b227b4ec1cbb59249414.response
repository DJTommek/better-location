    <script>
(function () {
  var ul = document.getElementById('photos');
  var lis = ul.childNodes;
  var removed = 0;
  for (var i = 0, ii = lis.length; i < ii - removed; ++i) {
    if (lis[i].nodeType !== 1) {
      ul.removeChild(lis[i]);
      removed += 1;
    }
  }

  var form = document.getElementById('add-photo-form');
  var link = document.getElementById('add-photo-link');
  var input = document.getElementById('add-photo-input');
  var preview = document.getElementById('add-photo-preview');
  var loader = document.getElementById('add-photo-loader');

  input.onchange = function () {
    var files = input.files;
    if (files) {
      var file = files[0];
      if (file.size > 10 * 1024 * 1024) {
        alert('Maximální povolená velikost obrázku je 10 MB. Zmenšete obrázek a zkuste jej nahrát znovu.');
        input.value = '';
        preview.style.display = 'none';
        return;
      }
      var reader = new FileReader();
      reader.onloadend = function (e) {
        var img = new Image();
        img.src = e.target.result;
        preview.innerHTML = '';
        preview.appendChild(img);
        preview.style.display = 'block';

        img.onload = function () {
          img.onload = null;
          if (form.scrollIntoView) {
            form.scrollIntoView();
          }
        };
      };
      reader.readAsDataURL(file);
    }
  };

  link.onclick = function (e) {
    form.style.display = 'block';
    if (form.scrollIntoView) {
      form.scrollIntoView();
    }
    return false;
  };
  form.onsubmit = function () {
    if (!input.value) {
      return false;
    }

    preview.style.display = 'none';
    form.style.display = 'none';
    loader.style.display = 'block';

    window.setTimeout(function () {
      for (var i = 0, ii = form.length; i < ii; ++i) {
        form[i].value = '';
      }
    }, 0);
  };
}());
function handleUploadedPhotos(results) {
  document.getElementById('add-photo-loader').style.display = 'none'

  var result;
  for (var i = 0, ii = results.length; i < ii; ++i) {
    result = results[i];
    if (result['error']) {
      alert('Vaše fotografie nebyla přidána (chyba ' + result['error'] + ')');
    } else {
      pushPhoto(result);
            alert('Vaše fotografie byla přidána a čeká na schválení.');
          }
  }
};
function pushPhoto(photo) {
  var noPhotos = document.getElementById('no-photos');
  if (noPhotos) {
    noPhotos.parentNode.removeChild(noPhotos);
  }

  var ul = document.getElementById(photo['zobrazit'] ? 'photos' : 'photos-queue');
  var li = document.createElement('li');

  var size_qs = photo['zobrazit'] ? (ul.childNodes.length ? 'w=78&h=78' : 'w=240') : 'w=78&h=78';

  if (!photo['zobrazit']) {
    document.getElementById('photos-queue-title').style.display = 'block';
  }

  var a = document.createElement('a');
  a.className = 'image-browser-source';
  a.href = './image.php?id=' + photo['id'] + '&h=800';
  a.title = photo['popis'];
  a.setAttribute('data-browser', 'photos');

  var note = [];
  if (photo['datum']) {
    note.push('datum pořízení fotografie: ' + photo['datum']);
  }
  if (photo['zdroj']) {
    note.push('zdroj: ' + photo['zdroj']);
  }
  if (photo['prispel'] && photo['prispel'] !== photo['zdroj']) {
    note.push('nahrál: ' + photo['prispel']);
  }
  a.setAttribute('data-note', note.join(' – '));

  var img = new Image();
  img.src = './image.php?id=' + photo['id'] + '&' + size_qs;
  a.appendChild(img);
  li.appendChild(a);
  ul.appendChild(li);

  ui.ImageBrowser.findSetsOnPage();
};

