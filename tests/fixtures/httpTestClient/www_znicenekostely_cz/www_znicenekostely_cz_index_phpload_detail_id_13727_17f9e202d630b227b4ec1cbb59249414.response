kostely.removeComment = function (self) {
  if (!confirm('Chcete opravdu daný komentář smazat?')) {
    return false;
  }
  setTimeout(function () {
    self.parentNode.parentNode.parentNode.removeChild(self.parentNode.parentNode);
  }, 1000);
};

kostely.pushComment = function (comment) {
  var comments = document.getElementById('comments');
  var form_child = comments.lastChild;
  if (form_child.tagName !== 'LI') {
    form_child = form_child.previousSibling;
  }

  var li = document.createElement('LI');
  var span = document.createElement('SPAN');
  span.innerHTML = '<strong>' + comment['user_name'] + '</strong> napsal ' + comment['date'];
  li.appendChild(span);
  li.id = 'comment' + comment['id'];
  li.innerHTML += comment['text'];
  comments.insertBefore(li, form_child);

  window.location.hash = '#comment' + comment['id'];

  var form = document.getElementById('comment-form');
  if (form) {
    form.onsubmit = function () {
      if (form['text'].value === '') {
        alert('Nelze přidat prázdný komentář.');
        return false;
      }
    };
    setTimeout(function () {
      form['text'].value = '';
    }, 0);
  }
};
