		form['okres'].disabled = val;
		form['okres1938'].disabled = val;
		form['kraj'].disabled = val;
		form['zeme'].disabled = val;
	};

	var updateClassName = function (input) {
		input = (input && input.tagName === 'INPUT') ? input : this;
		input.className = input.value ? 'has-value' : '';
	};
	var observeValue = function (input) {
		updateClassName(input);
		input.onblur = updateClassName;
	};
	observeValue(form['lokalita']);
	observeValue(form['query']);
	observeValue(form['obec']);
	observeValue(form['object_id']);
	observeValue(form['kod_cz']);

	}());
